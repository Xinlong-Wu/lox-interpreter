// CHECK-PARSER-NEXT:      Call: [Variable: [print](Variable: [a], )];
// CHECK-PARSER-NEXT:      Call: [Variable: [print](Variable: [c], )];

var a = "before";
var c = a = "var";
print (a); // expect: var
print (c); // expect: var
