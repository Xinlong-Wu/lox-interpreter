// RUN: %parser %s 2>&1 | FileCheck %s

// CHECK: Base.method()
// CHECK: Base.method()
class Base {
  method() {
    print "Base.method()";
  }
}

class Derived < Base {
  method() {
    super.method();
  }
}

class OtherBase {
  method() {
    print "OtherBase.method()";
  }
}

var derived = Derived();
derived.method(); // expect: Base.method()

Base = OtherBase;
derived.method(); // expect: Base.method()
